; args: vehicle
_vehicle = _this

? !local _vehicle: hint "Remote Vehicle. Deploy won't change.", exit

_deploy = [0, 1] select (fuel _vehicle > 0)


? "M109A6G_xj200" countType [_vehicle] > 0: [_vehicle, _deploy] exec "Player\Effect\DeployM109A6G.sqs"; exit
? "PLZ05_TZK_xj200" countType [_vehicle] > 0: [_vehicle, _deploy] exec "Player\Effect\DeployPLZ05.sqs"; exit
? "TOS1_TZK_xj200" countType [_vehicle] > 0: [_vehicle, _deploy] exec "Player\Effect\DeployTOS1.sqs"; exit

; EOF