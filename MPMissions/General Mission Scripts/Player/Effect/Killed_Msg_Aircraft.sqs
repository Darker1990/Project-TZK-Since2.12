@ !isNull pvKilledAircraft || _time > _timeAbort
? isNull pvKilledAircraft: [] exec "Player\Effect\Killed_Msg_Aircraft.sqs"; exit
_vehicle = pvKilledAircraft
pvKilledAircraft = objNull

[_vehicle] exec {Player\Effect\Killed_csla_burn_Unit_Air.sqs};

[] exec "Player\Effect\Killed_Msg_Aircraft.sqs"; exit

goto "Start"