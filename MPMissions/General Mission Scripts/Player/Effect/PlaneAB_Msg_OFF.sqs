; Effects won't work on dedicated server.
? isServer && isNull player : exit
_timeAbort = (1 + random 2) * 60

#Start
@ !isNull pvAfterBurnerOFF || _time > _timeAbort
? isNull pvAfterBurnerOFF: [] exec "Player\Effect\PlaneAB_Msg_OFF.sqs"; exit
_plane = pvAfterBurnerOFF; pvAfterBurnerOFF = objNull

BurningPlanes = BurningPlanes - [_plane]

[] exec "Player\Effect\PlaneAB_Msg_OFF.sqs"; exit

goto "Start"