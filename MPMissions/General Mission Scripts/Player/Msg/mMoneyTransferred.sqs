_timeAbort = (1 + random 2) * 60
#Start

@ pvMoneyTransfer != -1 || _time > _timeAbort
? pvMoneyTransfer == -1: [] exec "Player\Msg\mMoneyTransferred.sqs"; exit

_amount = pvMoneyTransfer

@ pvMoneyTransferGroup != -1
_value = pvMoneyTransferGroup

pvMoneyTransfer = -1
pvMoneyTransferGroup = -1

[_amount, _value] exec "Player\Msg\hMoneyTransferred.sqs"

[] exec "Player\Msg\mMoneyTransferred.sqs"; exit

goto "Start"
