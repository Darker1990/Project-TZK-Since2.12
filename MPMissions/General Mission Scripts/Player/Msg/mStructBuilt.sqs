_timeAbort = (1 + random 2) * 60
#Start

@ pvStructBuilt != -1 || _time > _timeAbort
? pvStructBuilt == -1: [] exec "Player\Msg\mStructBuilt.sqs"; exit

_value = pvStructBuilt
_object = pvStructBuilt_Object

pvStructBuilt = -1
[_value, _object] exec "Player\Msg\hStructBuilt.sqs"

[] exec "Player\Msg\mStructBuilt.sqs"; exit

goto "Start"
