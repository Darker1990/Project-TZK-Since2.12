; args: [_amount, _value]
_amount = _this select 0; _value = _this select 1

_giFrom = _value % GroupsNum
_value = (_value - _giFrom) / GroupsNum

_giTo = _value % GroupsNum
_value = (_value - _giTo) / GroupsNum

_si = _value

? _giFrom == _giTo && _giFrom == giCO select _si: _giList = [_si] call funcGetAIGroupsIndex; {[_si, _giFrom, _x, _amount] exec "Server\MoneyTransfer.sqs"} forEach _giList; exit

[_si, _giFrom, _giTo, _amount] exec "Server\MoneyTransfer.sqs"