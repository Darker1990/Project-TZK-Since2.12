_timeAbort = (1 + random 2) * 60
#Start

@ pvMoneyGive != -1 || _time > _timeAbort
? pvMoneyGive == -1: [] exec "Server\Msg\mGiveMoney.sqs"; exit

_amount = pvMoneyGive

@ pvMoneyGiveGroup != -1
_value = pvMoneyGiveGroup

pvMoneyGive = -1
pvMoneyGiveGroup = -1

[_amount, _value] exec "Server\Msg\hGiveMoney.sqs"

[] exec "Server\Msg\mGiveMoney.sqs"; exit

goto "Start"